{
	"prefix": "cache_",
	"handler": "",
	"status": [
		200
	],
	"default_expire": 86400,
	"rules": [
		{
			"name": "Home",
			"group": "static",
			"route": "/test",
			"regex": "test/[0-9]+",
			"ignore_case": false,
			"expire": 10,
			"methods": [ "OPTIONS", "GET", "HEAD", "POST", "PUT", "DELETE", "TRACE", "CONNECT" ],
			"variables": {
				"params": [ "product_id", "id" ],
				"query": [
					"get_var",
					{
						"value": "search",
						"ignore_case": true
					},
					"abc"
				],
				"cookies": [ "cookie_name" ],
				"body": [ "post_var", "username" ],
				"session": [ "post_var", "username" ],
				"header": [ "Authorization" ],
				"subdomains": [ "static" ]
			}
		}
	]
}